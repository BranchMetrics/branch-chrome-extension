{
  "manifest_version": 3,

  "name": "Branch Link Creator",
  "description": "This extension is to be used to create Branch links from your Chrome browser.",
  "version": "3.0",

  "action": {
    "default_icon": "icon.png",
    "default_popup": "home.html"
  },
  "icons": { 
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://api.branch.io/"
  ]
}
